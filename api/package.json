{
  "name": "proprly-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "check": "npm run format && npm run lint -- --fix && npm run typecheck"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#types/*": "./types/*.js",
    "#helper/*": "./helper/*.js",
    "#constants/*": "./constants/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.1",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.1.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.22",
    "@types/axios": "^0.9.36",
    "@types/lodash": "^4.17.18",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pdfkit": "^0.17.3",
    "eslint": "^9.9.1",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/bouncer": "^3.1.3",
    "@adonisjs/cache": "^1.3.0",
    "@adonisjs/core": "^6.17.2",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/i18n": "^2.2.1",
    "@adonisjs/limiter": "^2.4.0",
    "@adonisjs/lucid": "^21.2.0",
    "@adonisjs/mail": "^9.2.2",
    "@aws-sdk/client-s3": "^3.658.0",
    "@aws-sdk/client-sns": "^3.654.0",
    "@aws-sdk/client-sqs": "^3.713.0",
    "@aws-sdk/s3-request-presigner": "^3.817.0",
    "@vinejs/vine": "^2.1.0",
    "axios": "^1.8.4",
    "edge.js": "^6.2.1",
    "firebase-admin": "^13.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "ua-parser-js": "^2.0.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    }
  },
  "prettier": "@adonisjs/prettier-config"
}
